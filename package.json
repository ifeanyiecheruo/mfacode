{
  "name": "mfacode",
  "version": "2.0.4",
  "description": "Authentication code generator",
  "author": "Ifeanyi Echeruo (https://ifeanyi.echeruo.net)",
  "license": "MIT",
  "keywords": [
    "rfc",
    "6238",
    "otp",
    "totp",
    "one time password",
    "authenticator"
  ],
  "repository": "github:ifeanyiecheruo/mfacode",
  "bugs": {
    "url": "https://github.com/ifeanyiecheruo/mfacode/issues"
  },
  "bin": {
    "mfacode": "bin/index.js"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "doc": "doc",
    "src": "src"
  },
  "files": [
    "README.md",
    "bin/**/*.js",
    "lib/**/*.*",
    "doc/**/*.*",
    "src/**/*.ts",
    "src/**/*.json"
  ],
  "scripts": {
    "prepare": "webpack --config src/webpack.config.js --optimization-minimize"
  },
  "dependencies": {
    "otp": "^1.0.1",
    "xkeychain": "^0.0.6"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/webpack": "^4.41.22",
    "expect": "^26.5.3",
    "mocha": "^8.1.3",
    "name-all-modules-plugin": "^1.0.1",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2",
    "webpack-permissions-plugin": "^1.0.7"
  }
}
